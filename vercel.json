{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "sleeper-lineup-watch-ui/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}