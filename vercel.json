{
  "version": 2,
  "buildCommand": "cd sleeper-lineup-watch-ui && npm install && npm run build",
  "outputDirectory": "sleeper-lineup-watch-ui/dist",
  "functions": {
    "api/**": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}