{
  "version": 2,
  "builds": [
    {
      "src": "sleeper-lineup-watch-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/sleeper/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/sleeper-lineup-watch-ui/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/sleeper-lineup-watch-ui/dist/index.html"
    }
  ]
}